{
  "name": "interlinked-artofstrategy",
  "version": "1.0.0",
  "description": "Interlinked Art of Strategy - A platform connecting people and ideas through strategic analysis",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "dev:backend": "cd backend && npm run dev",
    "build:backend": "cd backend && npm run build",
    "start:backend": "cd backend && npm start",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "start:frontend": "cd frontend && npm start",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test"
  },
  "keywords": [
    "strategy",
    "neo4j",
    "graph-database",
    "typescript",
    "react",
    "business-intelligence"
  ],
  "author": "Interlinked Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElSancturioDeThomas/INTERLINKED-ARTOFSTRATEGY.git"
  }
}
